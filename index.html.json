lass=\"hamburger-btn \">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav class=\"\">\n            <ul>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    document.querySelector('.hamburger-menu_1714783845171_47').classList.remove('open');\n    document.querySelector('.hamburger-menu_1714783845171_47').classList.add('closed');\n  }\n  function open() {\n    isOpen = true;\n    document.querySelector('.hamburger-menu_1714783845171_47').classList.add('open');\n    document.querySelector('.hamburger-menu_1714783845171_47').classList.remove('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  function scrollOrResize() {\n    close();\n    move();\n  }\n  var silexElement = document.querySelector('.hamburger-menu_1714783845171_47').closest('.editable-style');\n  function move() {\n    var data = window.silex\n    var rect = silexElement.getBoundingClientRect();\n    var offset = {\n      top: rect.top + data.scrollTop,\n      left: rect.left + data.scrollLeft,\n    };\n    var container = document.querySelector('.hamburger-menu_1714783845171_47 .nav-container');\n    container.style.transform = 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)';\n  }\n  document.querySelector('.hamburger-menu_1714783845171_47 .hamburger-btn').addEventListener('click', function() { toggle(); });\n  document.addEventListener('resize', scrollOrResize);\n  document.addEventListener('scroll', scrollOrResize);\n  scrollOrResize()\n  setTimeout(() => scrollOrResize(), 0)\n})();\n</script>\n"}}},"innerHtml":"<form id=\"id_1714785066073_188\" action=\"https://formspree.io/your@email.com\" method=\"POST\" >\n  \n  <div>\n    <label for=\"field1\">Name</label